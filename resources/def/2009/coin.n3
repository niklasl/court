@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix : <http://purl.org/court/def/2009/coin#> .


: a owl:Ontology;
    rdfs:label "The Coin Ontology"@en;
    rdfs:comment """Coin describes schemes for URI construction."""@en;
    dct:created "2009-11-28"^^xsd:date;
    dct:modified "2010-04-06"^^xsd:date;
    dct:creator <http://purl.org/NET/dust/foaf#self> .


:CoinScheme a owl:Class;
    rdfs:label "Coin Scheme"@en;
    rdfs:comment """A scheme describing how to construct URI:s for
            resources based on their component properties."""@en;
    rdfs:isDefinedBy : .

:root a owl:DatatypeProperty;
    rdfs:label "root"@en;
    rdfs:domain :CoinScheme;
    rdfs:isDefinedBy : .

:separator a owl:DatatypeProperty;
    rdfs:label "separator"@en;
    rdfs:domain :CoinScheme;
    rdfs:isDefinedBy : .

:fragmentSeparator a owl:DatatypeProperty;
    rdfs:label "fragment separator"@en;
    rdfs:domain :CoinScheme;
    rdfs:isDefinedBy : .


:component a owl:ObjectProperty;
    rdfs:label "component"@en;
    rdfs:range rdf:Property;
    rdfs:isDefinedBy : .

:fragmentPrefix a owl:DatatypeProperty;
    rdfs:label "fragment prefix"@en;
    rdfs:isDefinedBy : .

:segment a owl:DatatypeProperty;
    rdfs:label "segment"@en;
    rdfs:isDefinedBy : .

#:prefix a owl:DatatypeProperty;
#    rdfs:label "prefix"@en;
#    rdfs:isDefinedBy : .


:Containment a owl:Class;
    rdfs:label "Containment"@en;
    rdfs:comment """Containment by relation to base resource."""@en;
    rdfs:isDefinedBy : .

:baseRel a owl:ObjectProperty;
    rdfs:label "base relation"@en;
    rdfs:comment """References a property which the contained resource is a
            subject of. The object of that will be used as the base URI for
            this containment."""@en;
    rdfs:domain :Containment;
    rdfs:range rdf:Property;
    rdfs:isDefinedBy : .

:baseRev a owl:ObjectProperty;
    rdfs:label "base reverse relation"@en;
    rdfs:comment """References a property which the contained resource is an
            object of. The subject of that will be used as the base URI for
            this containment."""@en;
    rdfs:domain :Containment;
    rdfs:range rdf:Property;
    rdfs:isDefinedBy : .


:Base a owl:Class;
    rdfs:label "Base"@en;
    rdfs:isDefinedBy : .

:scheme a owl:ObjectProperty;
    rdfs:label "scheme"@en;
    rdfs:domain :Base;
    rdfs:range :CoinScheme;
    rdfs:isDefinedBy : .

:contains a owl:ObjectProperty;
    rdfs:label "contains"@en;
    rdfs:domain :Base;
    #rdfs:range :Base; # Part?
    rdfs:isDefinedBy : .

:useTokenSet a owl:ObjectProperty;
    rdfs:label "base"@en;
    rdfs:domain :Base;
    rdfs:range :TokenSet;
    rdfs:isDefinedBy : .

:forType a owl:ObjectProperty;
    rdfs:label "for type"@en;
    rdfs:range rdfs:Class;
    rdfs:isDefinedBy : .

:TokenSet a owl:Class;
    rdfs:label "Token set"@en;
    rdfs:isDefinedBy : .

:Token a owl:Class;
    rdfs:label "Token"@en;
    rdfs:isDefinedBy : .

:tokenFrom a owl:ObjectProperty;
    rdfs:label "token from"@en;
    rdfs:domain :Token;
    rdfs:range :Property;
    rdfs:isDefinedBy : .

:tokenSet a owl:ObjectProperty;
    rdfs:label "token set"@en;
    rdfs:domain :Token;
    rdfs:range :TokenSet;
    rdfs:isDefinedBy : .

:represents a owl:ObjectProperty;
    rdfs:label "represents"@en;
    rdfs:domain :Token;
    rdfs:isDefinedBy : .


:TokenTranslation a owl:Class;
    rdfs:label "Token translation"@en;
    rdfs:isDefinedBy : .

:UrlEncodedTranslation a owl:Class;
    rdfs:label "URL-encoded translation"@en;
    rdfs:subClassOf :TokenTranslation;
    rdfs:isDefinedBy : .

:UpperCasedTranslation a owl:Class;
    rdfs:label "Upper-cased translation"@en;
    rdfs:subClassOf :TokenTranslation;
    rdfs:isDefinedBy : .

:LowerCasedTranslation a owl:Class;
    rdfs:label "Lower-cased translation"@en;
    rdfs:subClassOf :TokenTranslation;
    rdfs:isDefinedBy : .

:BaseCharTranslation a owl:Class;
    rdfs:label "Base char translation"@en;
    rdfs:subClassOf :TokenTranslation;
    rdfs:isDefinedBy : .

:tokenTranslation a owl:DatatypeProperty;
    rdfs:label "token translation"@en;
    rdfs:range :TokenTranslation;
    rdfs:isDefinedBy : .

:spaceReplacement a owl:DatatypeProperty;
    rdfs:label "space replacement"@en;
    rdfs:domain :TokenTranslation;
    rdfs:isDefinedBy : .

:stripRegexp a owl:DatatypeProperty;
    rdfs:label "strip regexp"@en;
    rdfs:domain :TokenTranslation;
    rdfs:isDefinedBy : .

:replacementMap a owl:ObjectProperty;
    rdfs:label "replacement map"@en;
    rdfs:domain :TokenTranslation;
    rdfs:isDefinedBy : .

:baseCharMap a owl:ObjectProperty;
    rdfs:label "base char map"@en;
    rdfs:domain :BaseCharTranslation;
    rdfs:isDefinedBy : .

:validRegexp a owl:DatatypeProperty;
    rdfs:label "valid regexp"@en;
    rdfs:domain :TokenTranslation;
    rdfs:isDefinedBy : .

